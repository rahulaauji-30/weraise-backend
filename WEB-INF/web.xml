<web-app>
    <display-name>weraise</display-name>
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>com.main</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addcampagins</servlet-name>
        <servlet-class>com.database.Campaigns</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addcampagins</servlet-name>
        <url-pattern>/addcapmaigns</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addUsers</servlet-name>
        <servlet-class>com.security.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addUsers</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addcomment</servlet-name>
        <servlet-class>com.database.Comment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addcomment</servlet-name>
        <url-pattern>/comments</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>logIn</servlet-name>
        <servlet-class>com.security.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logIn</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>authenticate</servlet-name>
        <servlet-class>com.security.Authentication</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>authenticate</servlet-name>
        <url-pattern>/authenticate</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>posts</servlet-name>
        <servlet-class>Posts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>posts</servlet-name>
        <url-pattern>/post/*</url-pattern>
    </servlet-mapping>
</web-app>